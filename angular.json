{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cryptoGo-Community-Wizard": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/libs",
              "src/styles.css",
              "src/config/config.json",
			  "src/config/components.json",
			  "src/auth",
              {
                "glob": "**/*",
                "input": "node_modules/ngx-monaco-editor/assets/monaco",
                "output": "./assets/monaco/"
              },
              {
                "glob": "jquery.min.js",
                "input": "./node_modules/jquery/dist",
                "output": "./node_modules/jquery/dist"
              },
              {
                "glob": "popper.min.js",
                "input": "./node_modules/popper.js/dist/umd",
                "output": "./node_modules/popper.js/dist/umd"
              },
              {
                "glob": "bootstrap.min.js",
                "input": "./node_modules/bootstrap/dist/js",
                "output": "./node_modules/bootstrap/dist/js"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/trading-chart-designer",
                "output": "./node_modules/trading-chart-designer"
              }
            ],
            "styles": [
              "src/global-styles/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/global-styles",
                "src/modules",
                "src"
              ]
            },
            "scripts": [
              "src/libs/golden-layout/goldenlayout.min.js",
              "src/libs/chartjs/chartjs@2.7.3.min.js"
            ]
          },
          "configurations": {
            "stage": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/config/config.json",
                  "with": "src/config/stage-config.json"
                }
              ]
            },
            "prod": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/config/config.json",
                  "with": "src/config/prod-config.json"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "cryptoGo-Community-Wizard:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "cryptoGo-Community-Wizard:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cryptoGo-Community-Wizard:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/global-styles/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/global-styles",
                "src/modules",
                "src"
              ]
            },
            "assets": [
              "src/assets",
              "src/libs",
              "src/styles.css",
              "src/config"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "cryptoGo-Community-Wizard",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "",
      "styleext": "scss",
      "spec": false
    },
    "@schematics/angular:directive": {
      "prefix": "",
      "spec": false
    },
    "@schematics/angular:module": {
      "routing": true,
      "spec": false
    }
  }
}
